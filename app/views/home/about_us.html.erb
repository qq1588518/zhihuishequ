<script type="text/javascript">
    // JavaScript Document
    function dotClick(ulName,btnr,btnl,dotName,moveTime){

        /*在UI内部结尾处插入一个和第一个一样的li*/
        var myUl = $(ulName);
        var myli = $(ulName).children("li:first");
        var mylis = $(ulName).children("li");
        var oldot=$(dotName);
        var btnRight = $(btnr);
        var btnLeft = $(btnl);


       /* for(var i=0;i<mylis.length-1;i++)
         {
         oldot.append("<li></li>");
         }*/



        var aDotWidth = oldot.width();
        var aDotLiWidth = oldot.find("li").width();
        var oDotMr=(aDotWidth - aDotLiWidth*mylis.length)/mylis.length-1;
        oldot.css({"padding-left":oDotMr/2,"padding-right":oDotMr/2});
        oldot.width(aDotWidth -oDotMr);
        aDotWidth = oldot.width();
        var oDotLiMr =(aDotWidth - aDotLiWidth*mylis.length)/(mylis.length-1);

        oldot.find("li").css("margin-right",oDotLiMr);
        oldot.find("li:last").css("margin-right",0);
        var tbsc = null;
        var tips=$(myli).clone(true);
        myUl.append(tips);
        var mylilast = $(ulName).children("li:last");
        var mydotlilast = $(dotName).children("li:last");
        /*点击事件*/
        var num = 0;/*图片的*/
        var num2 = 0;/*小点点的*/
        var imgheight =myUl.children("li").css('width');/*获取插入图片的宽度*/
        imgheight = parseInt(imgheight)/*转义成数字*/
        var steps = myUl.children("li").length;/*获取li个数*/
        myUl.width(imgheight*steps)/*给ul赋值--宽度*/
        btnRight.click(function(){
            clearInterval(tbsc);
            num2++;
            num++;
            var distance=imgheight*-num;
            if(num > myUl.children("li:last").index()){
                num=1;
                myUl.css('left',0).stop().animate({left:(imgheight*-1)},300);
            }else{
                myUl.stop().animate({left:distance},300);
            };
            if(num2 > mydotlilast.index()){
                num2 = 0;
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }else{
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }
            tbsc=setInterval(sports,moveTime);
        })
        btnLeft.click(function(){
            clearInterval(tbsc);
            num2--;
            num--;
            var num1 = mylilast.index();
            var dist = imgheight*num*-1;
            if(num < 0){
                num = num1-1;
                myUl.css('left',(num1*-1*imgheight)).stop().animate({left:((num1-1)*-1*imgheight)},300)
            }else{
                myUl.stop().animate({left:dist},300);
            }
            if(num2 < 0){
                num2 = mydotlilast.index();
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }else{
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }
            tbsc=setInterval(sports,moveTime);
        })
        /*定时器*/
        function sports(){
            num2++;
            num++;
            var distance=imgheight*-num;
            if(num > myUl.children("li:last").index()){
                num=1;
                myUl.css('left',0).stop().animate({left:(imgheight*-1)},300);
            }else{
                myUl.stop().animate({left:distance},300);
            };
            if(num2 > mydotlilast.index()){
                num2 = 0;
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }else{
                oldot.children("li").eq(num2).addClass('currentdot').siblings().removeClass('currentdot');
            }
        }
        tbsc=setInterval(sports,moveTime);

        var distbts;
        oldot.children("li").click(function(){
            distbts=imgheight*$(this).index()*-1;
            $(this).addClass('currentdot').siblings().removeClass('currentdot');
            myUl.animate({left:distbts},300);
            num = $(this).index();
            num2 = $(this).index();
            clearInterval(tbsc);
            tbsc=setInterval(sports,moveTime);
        })
    }
    dotClick($(".sum"),$(".us_right"),$(".us_left"),$(".dots"),5000)

    /*公司高管部分  简介动画效果css*/
    $(".ceo").hover(function(){
        $(this).addClass("us_o100").siblings().removeClass("us_o100");
    },function(){
        $(this).removeClass("us_o100");
    })
</script>


